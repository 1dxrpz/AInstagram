<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    <title>Register - ainstagram</title>
    <link href="{{asset('static/libs/css/bootstrap.min.css')}}" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="{{asset('static/styles/index.css')}}">

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ" crossorigin="anonymous">


    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
    {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
    {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body>
    <div id="loader">
        <div class="loader_wrapper">
            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="297px" height="297px" viewBox="0 0 100 100" preserveAspectRatio="xMidYMid" style="display:block;" >
                <circle cx="50" cy="50" fill="none" stroke="#007bff" stroke-width="1" r="18" stroke-dasharray="84.82300164692441 30.274333882308138" transform="matrix(1,0,0,1,0,0)" ></circle>
            </svg>
        </div>
    </div>
    <nav class="navbar navbar-expand-lg bg-body-tertiary border border-bottom">
        <div class="container-fluid">
            <a class="navbar-brand logo" href="{{path("index")}}">
                AInstagram
            </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarText" aria-controls="navbarText" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarText">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link" href="{{path('index')}}">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{path('explore')}}">Explore</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{path('upload')}}">Upload</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{path('generate')}}">Generate</a>
                    </li>
                </ul>
                {% if not app.user %}
                <span class="navbar-text">
                    <button type="button" class="btn btn-outline-primary btn-sm">
                        <a href="{{path('login')}}" class="text-reset link-underline link-underline-opacity-0">login</a>
                    </button>
                    <button type="button" class="btn btn-primary btn-sm">
                        <a href="{{path('register')}}" class="text-reset link-underline link-underline-opacity-0">Register</a>
                    </button>
                </span>
                {% endif %}
                {% if app.user %}
                <span class="navbar-text">
                    <button type="button" class="btn btn-primary btn-sm">
                        <a href="{{path('account')}}" class="text-reset link-underline link-underline-opacity-0">My account</a>
                    </button>
                    <button type="button" class="btn btn-outline-danger btn-sm">
                        <a href="{{path('logout')}}" class="text-reset link-underline link-underline-opacity-0">Logout</a>
                    </button>
                </span>
                {% endif %}
            </div>
        </div>
    </nav>
    {% block body %}{% endblock %}
    <footer class="text-center text-lg-start bg-light text-muted border-top">
        <!-- Section: Links  -->
        <section class="">
            <div class="container text-center text-md-start mt-5">
                <!-- Grid row -->
                <div class="row mt-3">
                    <!-- Grid column -->
                    <div class="col-md-3 col-lg-4 col-xl-3 mx-auto mb-4">
                        <!-- Content -->
                        <h6 class="text-uppercase fw-bold mb-4">
                            <i class="fas fa-gem me-3"></i>AInstagram
                        </h6>
                        <p>
                            Lorem ipsum
                            dolor sit amet, consectetur adipisicing elit.
                        </p>
                    </div>
                    <!-- Grid column -->

                    <!-- Grid column -->
                    <div class="col-md-2 col-lg-2 col-xl-2 mx-auto mb-4">
                        <!-- Links -->
                        <h6 class="text-uppercase fw-bold mb-4">
                            Socials
                        </h6>
                        <p>
                            <a href="#!" class="text-reset">Twitter</a>
                        </p>
                        <p>
                            <a href="#!" class="text-reset">Instagram</a>
                        </p>
                        <p>
                            <a href="#!" class="text-reset">YouTube</a>
                        </p>
                        <p>
                            <a href="#!" class="text-reset">Facebook</a>
                        </p>
                    </div>
                    <!-- Grid column -->

                    <!-- Grid column -->
                    <div class="col-md-3 col-lg-2 col-xl-2 mx-auto mb-4">
                        <!-- Links -->
                        <h6 class="text-uppercase fw-bold mb-4">
                            Useful links
                        </h6>
                        <p>
                            <a href="{{path('explore')}}" class="text-reset">Explore</a>
                        </p>
                        <p>
                            <a href="{{path('account')}}" class="text-reset">Account</a>
                        </p>
                        <p>
                            <a href="{{path('settings')}}" class="text-reset">Settings</a>
                        </p>
                        <p>
                            <a href="{{path('generate')}}" class="text-reset">Generate</a>
                        </p>
                    </div>
                    <!-- Grid column -->

                    <!-- Grid column -->
                    <div class="col-md-4 col-lg-3 col-xl-3 mx-auto mb-md-0 mb-4">
                        <!-- Links -->
                        <h6 class="text-uppercase fw-bold mb-4">Contact</h6>
                        <p><i class="fas fa-home me-3"></i> Kazan, RU 12345, RU</p>
                        <p>
                            <i class="fas fa-envelope me-3"></i>
                            info@ainstagram.io
                        </p>
                        <p><i class="fas fa-phone me-3"></i> + 01 234 567 88</p>
                    </div>
                    <!-- Grid column -->
                </div>
                <!-- Grid row -->
            </div>
        </section>
        <!-- Section: Links  -->

        <!-- Copyright -->
        <div class="text-center p-4" style="background-color: rgba(0, 0, 0, 0.05);">
            © 2023 Copyright:
            <a class="text-reset fw-bold" href="{{path("index")}}">AInstagram.io</a>
        </div>
        <!-- Copyright -->
    </footer>

    <script src="{{asset('static/libs/js/jquery.min.js')}}"></script>
    <script src="{{asset('static/libs/js/popper.min.js')}}"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe" crossorigin="anonymous"></script>
    <script src="{{asset('static/libs/js/index.js')}}"></script>
    {% block scripts %}{% endblock %}
</body>
</html>
